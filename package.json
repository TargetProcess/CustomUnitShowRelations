{
    "name": "@targetprocess/custom_unit_show_relations",
    "version": "3.0.0",
    "description": "Show relations as arrows on a Board, List View and Timeline View",
    "author": "Aliaksei Shytkin <shitkin@targetprocess.com>",
    "contributors": [
        "Aliaksei Shytkin <shitkin@targetprocess.com>",
        "Sergey Mostovoy <mostovoy@targetprocess.com>",
        "Vladimir Tikhonov <tikhonov@targetprocess.com>"
    ],
    "scripts": {
        "start": "node scripts/startWatcher.js",
        "start:server": "NODE_ENV=development webpack-dev-server",
        "build": "npm run clean:dist && NODE_ENV=production webpack --progress",
        "build:development": "npm run clean:development && NODE_ENV=development webpack --output-path build/development",
        "lint": "npm run lint:eslint && npm run lint:tslint",
        "lint:eslint": "eslint \"**/*.{js,jsx}\"",
        "lint:tslint": "tslint --project tsconfig.json \"**/*.{ts,tsx}\"",
        "test": "jest",
        "clean:dist": "rimraf dist/*",
        "clean:development": "rimraf build/development/*"
    },
    "license": "MIT",
    "dependencies": {
        "lodash.isequal": "^4.5.0"
    },
    "devDependencies": {
        "@types/jquery": "^3.3.6",
        "@types/react": "^15.6.19",
        "@types/react-dom": "^15.5.8",
        "@types/underscore": "^1.8.9",
        "autoprefixer": "^9.1.3",
        "autoprefixer-core": "^6.0.1",
        "babel-core": "^6.26.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "combine-assets-plugin": "targetprocess/combine-assets-plugin",
        "css-loader": "^0.28.11",
        "cssnano": "^4.1.0",
        "eslint": "^5.4.0",
        "eslint-plugin-babel": "^5.1.0",
        "jest": "^23.5.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^3.0.0",
        "postcss-scss": "^2.0.0",
        "rimraf": "^2.6.2",
        "shelljs": "^0.5.3",
        "style-loader": "^0.23.0",
        "targetprocess-mashup-manifest-loader": "github:targetprocess/targetprocess-mashup-manifest-loader",
        "targetprocess-mashup-webpack-plugin": "targetprocess/targetprocess-mashup-webpack-plugin",
        "ts-jest": "^23.1.4",
        "ts-loader": "^3.5.0",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-react": "^3.6.0",
        "typescript": "^3.0.3",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.11.3",
        "webpack-merge": "^4.1.4"
    },
    "jest": {
        "testRegex": "\\.spec\\.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "modulePaths": [
            "<rootDir>"
        ]
    }
}
