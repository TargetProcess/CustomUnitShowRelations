{
    "name": "CustomUnitShowRelations",
    "version": "3.0.0",
    "description": "Show relations as arrows on a Board, List View and Timeline View",
    "keywords": [
        "relations",
        "targetprocess",
        "mashup"
    ],
    "author": "Aliaksei Shytkin <shitkin@targetprocess.com>",
    "scripts": {
        "start": "node scripts/startWatcher.js",
        "start:server": "cross-env NODE_ENV=development webpack-dev-server --config webpack/development.config.js",
        "build:development": "shx rm -rf build/development && cross-env NODE_ENV=development webpack --config webpack/development.config.js --output-path build/development",
        "build": "shx rm -rf build/library && cross-env NODE_ENV=production webpack --config webpack/library.config.js --output-path build/library --progress",
        "lint": "npm run lint:eslint && npm run lint:tslint",
        "lint:eslint": "eslint \"**/*.{js,jsx}\"",
        "lint:tslint": "tslint --project tsconfig.json \"**/*.{ts,tsx}\"",
        "test": "jest"
    },
    "license": "MIT",
    "dependencies": {
        "lodash.isequal": "^4.5.0",
        "tslib": "^1.9.3"
    },
    "devDependencies": {
        "@types/jquery": "^3.3.6",
        "@types/react": "^15.6.19",
        "@types/react-dom": "^15.5.8",
        "@types/underscore": "^1.8.9",
        "autoprefixer": "^9.1.3",
        "autoprefixer-core": "^6.0.1",
        "babel-core": "^6.26.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "combine-assets-plugin": "targetprocess/combine-assets-plugin",
        "cross-env": "^1.0.8",
        "css-loader": "^0.28.11",
        "eslint": "^5.4.0",
        "eslint-plugin-babel": "^5.1.0",
        "jest": "^23.5.0",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^3.0.0",
        "postcss-scss": "^2.0.0",
        "react": "^15.5.4",
        "shelljs": "^0.5.3",
        "shx": "^0.1.2",
        "style-loader": "^0.23.0",
        "targetprocess-mashup-manifest-loader": "github:targetprocess/targetprocess-mashup-manifest-loader",
        "targetprocess-mashup-webpack-plugin": "targetprocess/targetprocess-mashup-webpack-plugin",
        "ts-jest": "^23.1.4",
        "ts-loader": "^3.5.0",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "tslint-react": "^3.6.0",
        "tsml": "^1.0.1",
        "typescript": "^3.0.3",
        "underscore": "^1.8.3",
        "underscore-template-loader": "^0.5.1",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.11.3"
    },
    "contributors": [
        "Aliaksei Shytkin <shitkin@targetprocess.com>",
        "Sergey Mostovoy <mostovoy@targetprocess.com>",
        "Vladimir Tikhonov <tikhonov@targetprocess.com>"
    ],
    "jest": {
        "testRegex": "\\.spec\\.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "modulePaths": [
            "<rootDir>"
        ]
    }
}
